<p>
  <%- if controller_name != 'sessions' %>
    <%= link_to "Log in", new_session_path(resource_name), html_options = { id: "button"} %>
  <% end %>
</p>

<p>
  <%- if devise_mapping.registerable? && controller_name != 'registrations' %>
    <%= link_to "Sign up", new_registration_path(resource_name), html_options = { id: "button"} %>
  <% end %>
</p>
